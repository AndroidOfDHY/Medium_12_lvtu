<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="titlebar_title_style">
        <item name="android:textSize">23.0sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="BottomNavBarOfMain">
        <item name="android:button">@null</item>
        <item name="android:width">90.0dip</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16.0dip</item>
        <item name="android:background">@drawable/six_main_nav_btn_sel</item>
    </style>

    <style name="Mine_layout_botton">
        <item name="android:background">@drawable/six_settings_item_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20dip</item>
    </style>

    <style name="TrackDetailDataShowing">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/dp66</item>
    </style>

    <style name="TrackDetail">
        <item name="android:layout_width">@dimen/dp170</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="TrackDetailDataInfoIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:lines">2</item>
        <item name="android:textColor">@color/small_blue_text</item>
        <item name="android:textSize">9.0dip</item>
    </style>
    <style name="TrackDetailDataInfoLCDFont"></style>
    <style name="FontStyleUnitLcd"></style>
</resources>